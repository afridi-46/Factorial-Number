      "source": [
        "n = int(input(\"Enter input number : \"))\n",
        "\n",
        "fact = 1\n",
        "if n < 0:\n",
        "    print(\"Factorial number does not exist in negative numbers\")\n",
        "elif n == 0:\n",
        "    print(\"The factorial of 0 is equal to 1\")\n",
        "else:\n",
        "    for i in range(1, n + 1):\n",
        "        fact = fact * i\n",
        "    print(\"The factorial of\",n,\"is\",fact)\n"
      ],
